TS_RH {
  HBox { +-; "Leaf"; -+ };
  LeafT: (TS_Temp,6) C;

  HBox { +-; "Humidity"; -+ };
  RH_T  (SHT31_Temp,6) C;
  RH    (SHT31_RH,6) %;
}

TS_RH_Eng {
  HBox { +-; "Temp Sensor"; -+ };
  Addr:   (TS_Addr);
  Count:  (TS_Count);
  { Err: (TS_state,3) (TS_error,3) };
  OVFlow: (TS_ovflow,3);

  HBox { +-; SHT31; -+ };
  Status: (SHT31_Status,4);
  { Err: (SHT31_I2C_err,3) (SHT31_state,3) };
  CRC_err: (SHT31_CRC_err,3);
}
