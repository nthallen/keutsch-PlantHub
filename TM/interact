# Startup script for plant chamber experiment
FCC_USB=/dev/ttyUSB0

  Launch memo memo -o $Experiment.log -l4
  [ -z "$launch_err" ] && msg "Running script interact"
  Launch tm_bfr bfr -H bfr: -S:

  [ -e $FCC_USB ] &&
    Launch -    subbusd_serusb -b 115200 -p $FCC_USB -n SB -s SB
  Launch tm_gen plantcol
  Launch cmd    plantsrvr
# [ -e /dev/serusb2 ] &&
#   Launch - LICOR -d /dev/serusb2
  Launch -TMC-  lgr -N `mlf_find LOG`
  Launch -      tmdf
# Launch -TMC-  plantalgo
